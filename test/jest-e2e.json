{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@app/(.*)$": "<rootDir>/../src/app/$1",
    "^@infra/(.*)$": "<rootDir>/../src/infra/$1",
    "^@domain/(.*)$": "<rootDir>/../src/domain/$1",
    "^@test/(.*)$": "<rootDir>/../test/$1"
  }
  
}
